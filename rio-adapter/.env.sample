SERVICE_NAME=rio-adapter
ENV=uat
DATABASE_URL=postgres://checker:checker@localhost/db_checker?sslmode=disable
POLL_INTERVAL=10m
NATS_URL=nats://localhost:4222
REDIS_ADDR=localhost:6379
REDIS_DB=3
AWS_REGION=us-east-2
LOG_LEVEL=info
RIO_PORT=9010

CACHE_TTL=10m
CACHE_CLEANUP_FREQ=2m

INBOUND_SUBJECT=cmd.lp.quote_request.v1.RIO
OUTBOUND_SUBJECT=evt.lp.quote_response.v1.RIO

# Rio API Configuration
RIO_BASE_URL=https://app.sandbox.rio.trade/api
RIO_API_KEY=your-rio-api-key-here
# Or use AWS Secrets Manager (set secret name, leave RIO_API_KEY empty)
# RIO_API_KEY_SECRET=prod/rio-adapter/api-key
RIO_COUNTRY=US
RIO_POLL_INTERVAL=30s
RIO_WEBHOOK_URL=https://your-domain.com/webhooks/rio/orders
